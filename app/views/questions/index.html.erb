<h1>Questions</h1>
<% @questions.each do |item| %>
  <h2><%= item.title %></h2>
  <time datetime = "<%= item.formatted_created_at%>">
    <%= item.formatted_created_at %>
  </time>
  <p><%= item.body %>
    <br>
    <%= link_to 'show', question_path(item) %> |
    <%= link_to 'edit', edit_question_path(item) %> |
    <%= link_to 'Delete', question_path(item), data: { method: :delete, confirm: 'Are you sore?'} %>
  </p>
<% end %>
<p>Find me in app/views/questions/index.html.erb</p>
